# Automatically generated export makefile rules

CLASSES=$(SRCS:%.java=%.class)

all: $(DEST) $(SRCS) $(EXTERN_SRCS)
	$(JAVAC) -d $(DEST) $(JFLAGS) -classpath $(DEST) $(SRCS)
	$(JAVAC) -d $(DEST) $(JFLAGS) -classpath $(DEST):$(CP) $(EXTERN_SRCS)

$(DEST):
	-mkdir $(DEST)
